---
import KPIBand from './KPIBand.astro';
import LogoMarquee from './LogoMarquee.astro';
import ProcessSticky from './ProcessSticky.astro';

const {
  kpis = [
    { label: 'Returns Filed', value: 1200 },
    { label: 'Happy Clients', value: 350 },
    { label: 'Years Experience', value: 12 }
  ],
  logos = ['/img/logos/1.svg','/img/logos/2.svg','/img/logos/3.svg']
} = Astro.props;
---
<KPIBand items={kpis} />
<LogoMarquee logos={logos} />
<ProcessSticky />
